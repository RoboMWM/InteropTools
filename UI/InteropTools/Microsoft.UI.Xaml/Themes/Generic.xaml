<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="using:Microsoft.UI.Xaml.Controls"
    xmlns:preview="using:Microsoft.UI.Preview"
    xmlns:primitives="using:Microsoft.UI.Xaml.Controls.Primitives">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="RefreshContainerForegroundBrush" Color="#FFFFFFFF" />
            <SolidColorBrush x:Key="SwipeItemBackground" Color="{ThemeResource SystemAccentColor}" />
            <SolidColorBrush x:Key="RefreshVisualizerForeground" Color="#FFFFFFFF" />
            <SolidColorBrush x:Key="RefreshContainerBackgroundBrush" Color="#00FFFFFF" />
            <SolidColorBrush x:Key="RefreshVisualizerBackground" Color="#00FFFFFF" />
            <SolidColorBrush x:Key="SwipeItemUnexecutedBackground" Color="#FFA9A9A9" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="RefreshContainerForegroundBrush" Color="#FF000000" />
            <SolidColorBrush x:Key="SwipeItemBackground" Color="{ThemeResource SystemAccentColor}" />
            <SolidColorBrush x:Key="RefreshVisualizerForeground" Color="#FF000000" />
            <SolidColorBrush x:Key="RefreshContainerBackgroundBrush" Color="#00FFFFFF" />
            <SolidColorBrush x:Key="RefreshVisualizerBackground" Color="#00FFFFFF" />
            <SolidColorBrush x:Key="SwipeItemUnexecutedBackground" Color="#FFA9A9A9" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="HighContrast">
            <SolidColorBrush x:Key="RefreshContainerForegroundBrush" Color="{ThemeResource SystemColorHighlightTextColor}" />
            <SolidColorBrush x:Key="SwipeItemBackground" Color="{ThemeResource SystemAccentColor}" />
            <SolidColorBrush x:Key="RefreshContainerBackgroundBrush" Color="{ThemeResource SystemColorHighlightColor}" />
            <SolidColorBrush x:Key="SwipeItemUnexecutedBackground" Color="#FFA9A9A9" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    <controls:XamlIntegerToIndentationConverter x:Key="IntToIndConverter" />
    <controls:XamlBooleanToVisibilityConverter x:Key="XamlBooleanToVisibilityConverter" />
    <DataTemplate x:Key="TreeViewItemDataTemplate">

        <Grid Height="44">
            <Grid.Margin>
                <Binding Converter="{StaticResource IntToIndConverter}" Path="Depth" />
            </Grid.Margin>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="36" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.Children>
                <Grid Margin="12,0">
                    <Grid.Visibility>
                        <Binding Converter="{StaticResource XamlBooleanToVisibilityConverter}" Path="HasItems" />
                    </Grid.Visibility>
                    <Grid.Children>
                        <TextBlock
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Center"
                            FontFamily="Segoe MDL2 Assets"
                            FontSize="12"
                            Style="{ThemeResource BodyTextBlockStyle}"
                            Text="">
                            <TextBlock.Visibility>
                                <Binding
                                    Converter="{StaticResource XamlBooleanToVisibilityConverter}"
                                    Mode="OneWay"
                                    Path="IsExpanded" />
                            </TextBlock.Visibility>
                        </TextBlock>
                        <TextBlock
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Center"
                            FontFamily="Segoe MDL2 Assets"
                            FontSize="12"
                            Style="{ThemeResource BodyTextBlockStyle}"
                            Text="">
                            <TextBlock.Visibility>
                                <Binding
                                    Converter="{StaticResource XamlBooleanToVisibilityConverter}"
                                    ConverterParameter="Invert"
                                    Mode="OneWay"
                                    Path="IsExpanded" />
                            </TextBlock.Visibility>
                        </TextBlock>
                    </Grid.Children>
                </Grid>
                <TextBlock
                    Grid.Column="1"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Style="{ThemeResource BodyTextBlockStyle}">
                    <TextBlock.Text>
                        <Binding Path="Data" />
                    </TextBlock.Text>
                </TextBlock>
            </Grid.Children>
        </Grid>

    </DataTemplate>
