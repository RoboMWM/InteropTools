                                                    Value="{ThemeResource RepeatButtonRevealBackgroundPressed}" />
                                            <Setter Target="ContentPresenter.BorderBrush"
                                                    Value="{ThemeResource RepeatButtonRevealBorderBrushPressed}" />
                                            <Setter Target="ContentPresenter.Foreground"
                                                    Value="{ThemeResource RepeatButtonForegroundPressed}" />
                                        </VisualState.Setters>
                                    </VisualState>
                                    <VisualState x:Name="Disabled">
                                        <VisualState.Setters>
                                            <Setter Target="RootGrid.Background"
                                                    Value="{ThemeResource RepeatButtonRevealBackgroundDisabled}" />
                                            <Setter Target="ContentPresenter.BorderBrush"
                                                    Value="{ThemeResource RepeatButtonRevealBorderBrushDisabled}" />
                                            <Setter Target="ContentPresenter.Foreground"
                                                    Value="{ThemeResource RepeatButtonForegroundDisabled}" />
                                        </VisualState.Setters>
                                    </VisualState>
                                </VisualStateGroup.States>
