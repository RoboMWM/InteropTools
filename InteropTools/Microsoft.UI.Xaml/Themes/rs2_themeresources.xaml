                        <Grid.Children>
                            <Rectangle
                                x:Name="CheckedHighlightBackground"
                                Fill="{ThemeResource AppBarToggleButtonBackgroundChecked}"
                                Opacity="0" />
                            <Rectangle x:Name="AccentOverlayBackground"
                                       Fill="{ThemeResource AppBarToggleButtonRevealBackgroundHighLightOverlay}" />
