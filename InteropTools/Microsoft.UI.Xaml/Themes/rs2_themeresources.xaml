                                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                Value="{ThemeResource AppBarEllipsisButtonRevealBackgroundPressed}" />
                                                        </ObjectAnimationUsingKeyFrames.KeyFrames>
