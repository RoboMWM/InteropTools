                                                    Value="{ThemeResource AppBarToggleButtonForegroundPointerOver}" />
                                            <Setter Target="OverflowCheckGlyph.Foreground"
                                                    Value="{ThemeResource AppBarToggleButtonCheckGlyphForegroundPointerOver}" />
